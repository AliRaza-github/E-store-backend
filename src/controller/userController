const User = require("../model/userModel");

const register = async (req, res) => {
    const { userName } = req.body
    try {
        const userData = await User.findOne({ userName })
        if (userData) {
            return res.status(400).json({ error: error.message, data: null, message: "Username already exist" })
        }
        const user = new User({
            userName: userName
        })
        const savedUser = await user.save();
        console.log(" log 1 savedUser :",savedUser);
    }
    catch (error) {
        return res.status(500).json({ error: error.message || error, data: null, message: "Error in last" });
    }
}
module.exports={register}